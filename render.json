{
  "builds": [
    {
      "src": "app.py",
      "use": "@now/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/app.py"
    }
  ],
  "env": {
    "FLASK_ENV": "production"
  },
  "services": [
    {
      "id": "my-flask-app",
      "name": "flask",
      "installCommand": "pip install -r requirements.txt",
      "buildCommand": "",
      "startCommand": "python app.py"
    }
  ]
}
